{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "modules": "./modules",
          "styles": "./styles",
          "lib": "./lib"
        }
      }
    ]
  ]
}
